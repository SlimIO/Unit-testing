{
    "name": "@slimio/unit-testing",
    "version": "1.0.0",
    "description": "SlimIO Unit testing framework",
    "main": "index.js",
    "scripts": {
        "prepublishOnly": "pkg-ok",
        "test": "ava --verbose",
        "doc": "jsdoc -c ./jsdoc.json -r -R ./README.md -P ./package.json --verbose",
        "coverage": "nyc ava",
        "report": "nyc report --reporter=html"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/SlimIO/Unit-testing.git"
    },
    "keywords": [],
    "author": "SlimIO",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/SlimIO/Unit-testing/issues"
    },
    "homepage": "https://github.com/SlimIO/Unit-testing#readme",
    "dependencies": {
        "@slimio/is": "^1.5.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^8.1.0",
        "@commitlint/config-conventional": "^8.1.0",
        "@escommunity/minami": "^1.0.0",
        "@slimio/eslint-config": "^3.0.2",
        "@types/node": "^12.6.8",
        "ava": "^2.2.0",
        "eslint": "^6.1.0",
        "husky": "^3.0.2",
        "jsdoc": "^3.5.5",
        "nyc": "^14.0.0",
        "pkg-ok": "^2.3.1"
    },
    "engines": {
        "node": ">=10"
    },
    "husky": {
        "hooks": {
            "pre-push": "npm test",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "nyc": {
        "check-coverage": true,
        "per-file": false,
        "lines": 98,
        "statements": 98,
        "functions": 98,
        "branches": 90
    }
}
